{
  "name": "teddytags",
  "version": "0.2.0c",
  "description": "The superfast way to custom elements.",
  "jsdelivr": "./lib/browser/teddytags.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/obnoxiousnerd/teddytags.git"
  },
  "main": "./lib/teddytags.js",
  "files": [
    "lib/**/*"
  ],
  "bin": {
    "teddy": "bin/teddy"
  },
  "prettier": "./.prettierrc.js",
  "publishConfig": {
    "access": "public"
  },
  "types": "./lib/teddytags.d.ts",
  "prettyquick": {
    "*.{js,ts,yml,d.ts,md}": [
      "prettier ./.prettierrc.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "prepare": "tsc",
    "postinstall": "node lib/postinstall.js",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "pretty": "prettier .prettierrc.json",
    "test": "npm run prepare && mocha \"test/**/*.test.js\" --timeout 20000 --exit"
  },
  "keywords": [
    "custom elements",
    "teddytags",
    "obnoxoxiousnerd"
  ],
  "author": "Pranav Karawale",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "chai": "^4.2.0",
    "chai-webdriver": "^1.2.0",
    "chromedriver": "^79.0.2",
    "coveralls": "^3.0.9",
    "express": "^4.17.1",
    "geckodriver": "^1.19.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "mocha": "^7.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "typescript": "^3.7.5"
  }
}
