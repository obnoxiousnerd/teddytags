{
  "name": "teddytags",
  "version": "1.7.0",
  "description": "The superfast way to custom elements.",
  "type": "module",
  "browser": "./lib/teddytags.js",
  "jsdelivr": "./lib/teddytags.js",
  "main": "./lib/teddytags.js",
  "module": "./lib/teddytags.js",
  "exports": {
    "./": "./lib/",
    "./cjs": "./lib/cjs.js",
    "./legacy": "./lib/legacy.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obnoxiousnerd/teddytags.git"
  },
  "prettier": "./.prettierrc.json",
  "publishConfig": {
    "access": "public"
  },
  "types": "./lib/teddytags.d.ts",
  "prettyquick": {
    "*.{js,ts,yml,d.ts,md}": [
      "prettier ./.prettierrc.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:dts",
    "build:ts": "rollup --config rollup.config.ts",
    "build:ts:dev": "rollup --config rollup.config.ts --environment BUILD:production",
    "build:dts": "dts-bundle-generator --project src/tsconfig.json -o lib/teddytags.d.ts src/index.ts",
    "dev": "rollup --config rollup.config.ts --watch",
    "tsc:test": "tsc -p ./test",
    "cleanup:test": "rimraf test/**/*.js",
    "prepare": "npm run build",
    "test": "npm run build:dts && npm run build:ts:dev && npm run tsc:test && karma start karma.conf.ts && npm run cleanup:test"
  },
  "keywords": [
    "custom elements",
    "teddytags",
    "obnoxoxiousnerd"
  ],
  "author": "Pranav Karawale",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/jasmine": "^3.5.4",
    "@types/node": "^13.7.0",
    "@types/webpack": "^4.41.6",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "dts-bundle-generator": "^3.3.1",
    "husky": "^4.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-coverage": "^2.0.1",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-remap-coverage": "^0.1.5",
    "karma-webpack": "^4.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "terser": "^4.6.3",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6"
  }
}
