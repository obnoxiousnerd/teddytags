{
  "name": "teddytags",
  "version": "1.10.0",
  "description": "The superfast way to custom elements.",
  "browser": "./lib/teddytags.js",
  "jsdelivr": "./lib/teddytags.js",
  "main": "./lib/umd.js",
  "module": "./lib/teddytags.js",
  "exports": {
    "./": "./lib/",
    "./umd": "./lib/umd.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/obnoxiousnerd/teddytags.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./lib/teddytags.d.ts",
  "prettyquick": {
    "*.{js,ts,yml,d.ts,md}": [
      "prettier ./.prettierrc.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "alias:link": "tsc util/alias && node util/alias",
    "build": "npm run build:ts && npm run build:dts",
    "build:dev": "npm run build:ts:dev && npm run build:dts",
    "build:ts": "rollup --config rollup.config.ts",
    "build:ts:dev": "rollup --config rollup.config.ts --environment BUILD:dev",
    "build:dts": "npm run build:dts:es && npm run build:dts:jsx",
    "build:dts:es": "dts-bundle-generator --project src/tsconfig.json -o lib/teddytags.d.ts src/index.ts",
    "build:dts:jsx": "tsc util/buildjsxdefs && node util/buildjsxdefs",
    "dev": "rollup --config rollup.config.ts --watch",
    "prepare": "npm run build",
    "test": "npm run alias:link && npm run build:dev && karma start karma.conf.ts"
  },
  "keywords": [
    "custom elements",
    "teddytags",
    "obnoxoxiousnerd"
  ],
  "author": "Pranav Karawale",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/jasmine": "^3.5.4",
    "@types/node": "^13.7.0",
    "coveralls": "^3.0.9",
    "dts-bundle-generator": "^4.0.0",
    "husky": "^4.2.1",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-coverage": "^2.0.1",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-remap-coverage": "^0.1.5",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.2"
  }
}
